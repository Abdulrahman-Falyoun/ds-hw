{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "monorepo": true,
  "root": "apps/gateway",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "projects": {
    "image-handler": {
      "type": "application",
      "root": "apps/image-handler",
      "entryFile": "main",
      "sourceRoot": "apps/image-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/image-handler/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "website-handler": {
      "type": "application",
      "root": "apps/website-handler",
      "entryFile": "main",
      "sourceRoot": "apps/website-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/website-handler/tsconfig.app.json"
      }
    },
    "mail-handler": {
      "type": "application",
      "root": "apps/mail-handler",
      "entryFile": "main",
      "sourceRoot": "apps/mail-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-handler/tsconfig.app.json"
      }
    }
  }
}